{
  "bindings": [
    {
      "name": "pdfStream",
      "type": "blobTrigger",
      "direction": "in",
      "path": "pdfblob/{name}",
      "connection": "AzureWebJobsDashboard"
    },
    {
      "type": "blob",
      "name": "$return",
      "path": "textblob/{rand-guid}",
      "connection": "AzureWebJobsDashboard",
      "direction": "out"
    }
  ],
  "disabled": false
}